<div id='prods'>
<div class="container">
  
             
 

  <form id='pf' action='products' method='post'>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Price</th>        
        <th>Category</th>
      </tr>
    </thead>
    <tbody>
    {{#each products}}

      <tr id='{{prodID}}'>      
        <td>
        <input type="text" name="name" value="{{name}}">        
        <div class="glyphicon glyphicon-pencil"></div>
        </td>        

        <td>
        <input type="text" name="price" value="{{price}}">
        <div class="glyphicon glyphicon-pencil"></div>        
        </td>   

        <td><input type="text" name="category" value="{{category}}">        
        </td>       

      </tr>

        

      {{/each}}
      </tbody>
  </table>
  </form>
</div>
</div>

















