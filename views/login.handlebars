<html>
	<head>
	<title> Login </title>
	<link rel='stylesheet' type='text/css' href='register.css'>
	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script type='text/javascript' src='login.js'></script>
	</head>
 		
	<body>

		<div class='container' id ='main'>
			<form action='/login' method='POST'>
			<h1> <b>Nelisa's Spazashop</b></h1>
			<br>
			<h3><span class="label ">Username</span></h3>
			<input type='text' name='name' id='username' >
			<br>
		
			<h3><span class="label ">Password</span></h3>
			<input type='password' name='password' id='pass' >
			<br>
			
					
			<button type='button' id="register" class='btn'>Register</button>
			<button type='button' id="done" class='btn' >Done</button>
			</form>
		</div>
		<script type='text/javascript'>

	 		var found = {{correct}}
	 		if(!found){
	 			popUp('Incorrect Login Details')
	 		}

 		</script>
	</body>

</html>